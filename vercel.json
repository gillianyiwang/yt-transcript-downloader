{
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "{api/**,styles/**,images/**,transcript_utils.py,app.py}",
      "excludeFiles": "{**/__pycache__/**,**/*.pyc,**/test*/**,**/*.dist-info/**,**/example*/**,**/doc*/**,**/*.egg-info/**,**/font*/**,**/*.ttf,**/*.ttc,**/*.md,**/pip/**,**/setuptools/**,**/pkg_resources/**,**/nodejs_wheel/**,**/pygments/**,**/reportlab/**}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
