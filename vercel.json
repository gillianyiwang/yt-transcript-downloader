{
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "{api/**,styles/**,images/**,transcript_utils.py,app.py}",
      "excludeFiles": "{**/__pycache__/**,**/*.pyc,**/*.pyo,**/tests/**,**/test/**,**/*.dist-info/**,**/examples/**,**/docs/**,**/*.egg-info/**,**/fonts/**,**/*.ttf,**/*.ttc,**/*.md,**/LICENSE*}"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
